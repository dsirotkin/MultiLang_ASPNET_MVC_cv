<!DOCTYPE html>
<!--[if IE 8]> <html lang="en" class="ie8"> <![endif]-->
<!--[if IE 9]> <html lang="en" class="ie9"> <![endif]-->
<!--[if !IE]><!-->
<html lang="en">
<!--<![endif]-->
<head>
    @Styles.Render("~/Content/print-css")
    <title>D.Sirotkin</title>
    <!-- Meta -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Responsive HTML5 Resume/CV Template for Developers">
    <meta name="author" content="Xiaoying Riley at 3rd Wave Media">
    <link rel="shortcut icon" href="~/content/clip.ico">
    <link href='https://fonts.googleapis.com/css?family=Roboto:400,500,400italic,300italic,300,500italic,700,700italic,900,900italic' rel='stylesheet' type='text/css'>

</head>
<body>
    <div class="wrapper">

        <div class="main-wrapper">

                <img class="profile" src="~/Content/profile.png" alt="" />
                <h1 class="name">@MultiLangCV.App_GlobalResources.CV.sb_name</h1>
                <h3 class="tagline">@MultiLangCV.App_GlobalResources.CV.sb_position</h3>

            <section class="section">
                <div class="about-container container-block">
                    <ul class="list-unstyled about-list">
                        <li>@MultiLangCV.App_GlobalResources.CV.sb_dob : 03.07.1990</li>
                        <li>@MultiLangCV.App_GlobalResources.CV.sb_region : @MultiLangCV.App_GlobalResources.CV.sb_region_name</li>
                    </ul>
                </div><!--//about-->

                <ul class="list-unstyled contact-list">
                    <h2 class="container-block-title">@MultiLangCV.App_GlobalResources.CV.sb_contacts</h2>
                    <li class="email"><i class="fa fa-envelope"></i><a href="mailto: it.sirotkin@gmail.com">it.sirotkin@gmail.com</a></li>
                    <li class="phone"><i class="fa fa-phone"></i><a href="tel:+380939033933">+38(093) 903 39 33</a></li>
                </ul>
                </section><!--//section-->

                <section class="section experiences-section">
                    <h2 class="section-title"><i class="fa fa-briefcase"></i>@MultiLangCV.App_GlobalResources.CV.mw_experiences</h2>

                    <div class="item">
                        <div class="meta">
                            <div class="upper-row">
                                <h3 class="job-title">@MultiLangCV.App_GlobalResources.CV.mw_3_exp_pos</h3>
                                <div class="time">@MultiLangCV.App_GlobalResources.CV.mw_3_exp_time</div>
                            </div><!--//upper-row-->
                            <div class="company">@Html.Raw(MultiLangCV.App_GlobalResources.CV.mw_3_exp_company)</div>
                        </div><!--//meta-->
                        <div class="details">
                            <p>@Html.Raw(MultiLangCV.App_GlobalResources.CV.mw_3_exp_resp)</p>
                        </div><!--//details-->
                    </div><!--//item-->

                    <div class="item">
                        <div class="meta">
                            <div class="upper-row">
                                <h3 class="job-title">@MultiLangCV.App_GlobalResources.CV.mw_2_exp_pos</h3>
                                <div class="time">@MultiLangCV.App_GlobalResources.CV.mw_2_exp_time</div>
                            </div><!--//upper-row-->
                            <div class="company">@Html.Raw(MultiLangCV.App_GlobalResources.CV.mw_2_exp_company)</div>
                        </div><!--//meta-->
                        <div class="details">
                            <p>@Html.Raw(MultiLangCV.App_GlobalResources.CV.mw_2_exp_resp)</p>
                        </div><!--//details-->
                    </div><!--//item-->

                    <div class="item">
                        <div class="meta">
                            <div class="upper-row">
                                <h3 class="job-title">@MultiLangCV.App_GlobalResources.CV.mw_1_exp_pos</h3>
                                <div class="time">@MultiLangCV.App_GlobalResources.CV.mw_1_exp_time</div>
                            </div><!--//upper-row-->
                            <div class="company">@Html.Raw(MultiLangCV.App_GlobalResources.CV.mw_1_exp_company)</div>
                        </div><!--//meta-->
                        <div class="details">
                            <p>@Html.Raw(MultiLangCV.App_GlobalResources.CV.mw_1_exp_resp)</p>
                        </div><!--//details-->
                    </div><!--//item-->


                </section><!--//section-->

                <section class="section summary-section">
                    <h2 class="section-title"><i class="fa fa-user"></i>@MultiLangCV.App_GlobalResources.CV.mw_add</h2>
                    <div class="summary">
                        <p>@Html.Raw(MultiLangCV.App_GlobalResources.CV.mw_add_info)</p>
                    </div><!--//summary-->
                </section><!--//section-->


                <section class="section education-section">
                    <h2 class="section-title"><i class="fa fa-book"></i>@MultiLangCV.App_GlobalResources.CV.mw_education</h2>
                    <div class="item">
                        <div class="meta">
                            <div class="upper-row">
                                <h3 class="edu-inst-title">@MultiLangCV.App_GlobalResources.CV.mw_edu_1_name</h3>
                                <div class="edu-time">@MultiLangCV.App_GlobalResources.CV.mw_edu_1_time</div>
                            </div><!--//upper-row-->
                            <div class="edu-inst-place">@Html.Raw(MultiLangCV.App_GlobalResources.CV.mw_edu_1_place)</div>
                        </div><!--//meta-->
                        <div class="details">
                            <p>@Html.Raw(MultiLangCV.App_GlobalResources.CV.mw_edu_1_rank)</p>
                        </div><!--//details-->
                    </div><!--//item-->
                    <div class="item">
                        <div class="meta">
                            <div class="upper-row">
                                <h3 class="edu-inst-title">@MultiLangCV.App_GlobalResources.CV.mw_edu_2_name</h3>
                                <div class="edu-time">@MultiLangCV.App_GlobalResources.CV.mw_edu_2_time</div>
                            </div><!--//upper-row-->
                            <div class="edu-inst-place">@Html.Raw(MultiLangCV.App_GlobalResources.CV.mw_edu_2_place)</div>
                        </div><!--//meta-->
                        <div class="details">
                            <p>@Html.Raw(MultiLangCV.App_GlobalResources.CV.mw_edu_2_rank)</p>
                        </div><!--//details-->
                    </div><!--//item-->
                </section><!--//section-->


                <section class="section languages-section">
                    <h2 class="section-title"><i class="fa fa-globe"></i>@MultiLangCV.App_GlobalResources.CV.mw_laguages</h2>
                    <div class="item">
                        <div class="details">
                            <p>@Html.Raw(MultiLangCV.App_GlobalResources.CV.mw_lang_en)</p>
                        </div><!--//details-->
                    </div><!--//item-->
                    <div class="item">
                        <div class="details">
                            <p>@Html.Raw(MultiLangCV.App_GlobalResources.CV.mw_lang_ua)</p>
                        </div><!--//details-->
                    </div><!--//item-->
                    <div class="item">
                        <div class="details">
                            <p>@Html.Raw(MultiLangCV.App_GlobalResources.CV.mw_lang_ru)</p>
                        </div><!--//details-->
                    </div><!--//item-->
                </section><!--//section-->


                <section class="section">
                    <h2 class="section-title"><i class="fa fa-rocket"></i>@MultiLangCV.App_GlobalResources.CV.mw_skills_title</h2>
                    <div class="skillset">
                        <div class="item">
                            <h2 class="level-title"> - MSSQL</h2>
                        </div><!--//item-->

                        <div class="item">
                            <h2 class="level-title"> - Firebird</h2>
                        </div><!--//item-->

                        <div class="item">
                            <h2 class="level-title"> - MySQL</h2>
                        </div><!--//item-->

                        <div class="item">
                            <h2 class="level-title"> - ASP.NET MVC</h2>
                        </div><!--//item-->

                        <div class="item">
                            <h2 class="level-title"> - ASP.NET WebForms</h2>
                        </div><!--//item-->

                        <div class="item">
                            <h2 class="level-title"> - HTML5 &amp; CSS</h2>
                        </div><!--//item-->

                        <div class="item">
                            <h2 class="level-title"> - Javascript</h2>
                        </div><!--//item-->

                        <div class="item">
                            <h2 class="level-title"> - Bootstrap</h2>
                        </div><!--//item-->

                        <div class="item">
                            <h2 class="level-title"> - Delphi</h2>
                        </div><!--//item-->

                    </div>
                </section><!--//skills-section-->

</div><!--//main-body-->
    </div>    
    <!-- Javascript -->
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")



</body>
</html>
